{
  "name": "kavindra",
  "version": "0.0.1",
  "scripts": {
    "lint": "concurrently -c \"green.bold,blue.bold\" yarn:lint:*",
    "lint:ui": "cd kavindra-ui && yarn run lint",
    "lint:api": "cd kavindra-api && yarn run lint",
    "start": "concurrently -c \"green.bold,blue.bold\" yarn:start:*",
    "start:ui": "cd kavindra-ui && yarn run start",
    "start:api": "cd kavindra-api && yarn run start",
    "test": "concurrently -c \"green.bold,blue.bold\" yarn:test:*",
    "test:ui": "cd kavindra-ui && yarn run test",
    "test:api": "cd kavindra-api && yarn run test",
    "prepare": "husky"
  },
  "keywords": [
    "Feedback"
  ],
  "author": "Jonathan Lee <jonathan.lee.devel@gmail.com>",
  "license": "GPL-3.0-or-later",
  "description": "Open-Source Feedback Platform",
  "devDependencies": {
    "concurrently": "^9.1.0",
    "husky": "^9.1.7"
  }
}
