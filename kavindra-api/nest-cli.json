{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kavindra-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "js-widget/**/*"
